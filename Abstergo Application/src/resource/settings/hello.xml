<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE preferences SYSTEM "http://java.sun.com/dtd/preferences.dtd">
<preferences EXTERNAL_XML_VERSION="1.0">
  <root type="user">
    <map/>
    <node name="setting">
      <map>
        <entry key="LogFile" value="Application;Security"/>
        <entry key="LogDisplay" value="Event Source;Event Type;Event ID;Event Time Generated"/>
        <entry key="IncomingAccept" value="true"/>
        <entry key="IncomingMaximum" value="90"/>
        <entry key="OnDisconnect" value="Logout"/>
        <entry key="Timeout" value="900000"/>
      </map>
    </node>
  </root>
</preferences>
