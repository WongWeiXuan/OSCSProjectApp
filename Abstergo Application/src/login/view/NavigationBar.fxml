<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXDrawer?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.TextFlow?>


<StackPane minHeight="850.0" minWidth="250.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="navigation.controller.NavigationBarController">
   <children>
      <JFXDrawer fx:id="jfxDrawer" defaultDrawerSize="250.0" minWidth="200.0" onDrawerClosing="#changeIcon" onDrawerOpening="#changeToWord">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0">
               <children>
                  <AnchorPane>
                     <children>
                        <ImageView fitWidth="210.0" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <image>
                              <Image url="@../../pictures/abstergo-industries-logo.png" />
                           </image>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </ImageView>
                     </children>
                     <VBox.margin>
                        <Insets left="20.0" right="20.0" />
                     </VBox.margin>
                  </AnchorPane>
                  <Line endX="125.0" startX="-125.0" stroke="WHITE" strokeLineCap="ROUND" strokeWidth="2.0" />
                  <TextFlow textAlignment="CENTER">
                     <children>
                        <Label style="-fx-font-family: &quot;Source Sans Pro&quot;;" text="Navigation 1" textFill="WHITE">
                           <font>
                              <Font size="24.0" />
                           </font>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Label>
                     </children>
                  </TextFlow>
                  <TextFlow textAlignment="CENTER">
                     <children>
                        <Label style="-fx-font-family: &quot;Source Sans Pro&quot;;" text="Navigation 2" textFill="WHITE">
                           <font>
                              <Font size="24.0" />
                           </font>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Label>
                     </children>
                  </TextFlow>
               </children>
               <padding>
                  <Insets top="20.0" />
               </padding>
            </VBox>
         </children>
      </JFXDrawer>
   </children>
</StackPane>
