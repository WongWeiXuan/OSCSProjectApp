<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.TextFlow?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1280.0" stylesheets="@FileStorage.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fileStorage.FileStorageController">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox prefHeight="70.0" prefWidth="1280.0" style="-fx-background-color: rgb(113,163,146);">
               <children>
                  <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../resource/pictures/abstergo-industries-logo.png" />
                     </image>
                     <HBox.margin>
                        <Insets left="10.0" top="5.0" />
                     </HBox.margin>
                  </ImageView>
               </children>
            </HBox>
            <HBox VBox.vgrow="ALWAYS">
               <children>
                  <JFXTreeTableView fx:id="fileTable" style="-fx-border-color: transparent;" HBox.hgrow="ALWAYS" />
                  <VBox prefHeight="650.0" prefWidth="250.0">
                     <children>
                        <StackPane>
                           <children>
                              <VBox fx:id="newWrap" alignment="CENTER">
                                 <children>
                                    <TextFlow textAlignment="CENTER">
                                       <children>
                                          <JFXButton fx:id="newBtn" alignment="CENTER" onAction="#showNewBtn" prefWidth="200.0" style="-fx-background-color: #0070e0;" text="New" textAlignment="CENTER" textFill="WHITE">
                                             <font>
                                                <Font name="System Bold" size="18.0" />
                                             </font>
                                             <cursor>
                                                <Cursor fx:constant="HAND" />
                                             </cursor>
                                          </JFXButton>
                                       </children>
                                    </TextFlow>
                                    <VBox fx:id="newList" disable="true" maxWidth="200.0" spacing="5.0" visible="false">
                                       <children>
                                          <Label text="Folder">
                                             <font>
                                                <Font name="System Bold" size="16.0" />
                                             </font>
                                             <cursor>
                                                <Cursor fx:constant="HAND" />
                                             </cursor>
                                          </Label>
                                          <Label fx:id="fileUploadOpt" onMouseClicked="#fileUpload" text="File upload">
                                             <font>
                                                <Font name="System Bold" size="16.0" />
                                             </font>
                                             <cursor>
                                                <Cursor fx:constant="HAND" />
                                             </cursor>
                                          </Label>
                                          <Label text="Folder upload">
                                             <font>
                                                <Font name="System Bold" size="16.0" />
                                             </font>
                                             <cursor>
                                                <Cursor fx:constant="HAND" />
                                             </cursor>
                                          </Label>
                                       </children>
                                       <VBox.margin>
                                          <Insets top="5.0" />
                                       </VBox.margin>
                                       <padding>
                                          <Insets left="5.0" />
                                       </padding>
                                    </VBox>
                                 </children>
                              </VBox>
                              <VBox fx:id="downloadWrap" alignment="CENTER" disable="true" visible="false">
                                 <children>
                                    <TextFlow textAlignment="CENTER">
                                       <children>
                                          <JFXButton fx:id="downloadBtn" alignment="CENTER" prefWidth="200.0" style="-fx-background-color: #0070e0;" text="Download" textFill="WHITE">
                                             <font>
                                                <Font name="System Bold" size="18.0" />
                                             </font>
                                             <cursor>
                                                <Cursor fx:constant="HAND" />
                                             </cursor>
                                          </JFXButton>
                                       </children>
                                    </TextFlow>
                                    <VBox maxWidth="200.0" spacing="5.0">
                                       <children>
                                          <Label fx:id="historyOpt" text="Version history">
                                             <font>
                                                <Font name="System Bold" size="18.0" />
                                             </font>
                                             <cursor>
                                                <Cursor fx:constant="HAND" />
                                             </cursor>
                                          </Label>
                                          <Label fx:id="moveOpt" text="Move">
                                             <font>
                                                <Font name="System Bold" size="18.0" />
                                             </font>
                                             <cursor>
                                                <Cursor fx:constant="HAND" />
                                             </cursor>
                                          </Label>
                                          <Label fx:id="copyOpt" text="Copy">
                                             <font>
                                                <Font name="System Bold" size="18.0" />
                                             </font>
                                             <cursor>
                                                <Cursor fx:constant="HAND" />
                                             </cursor>
                                          </Label>
                                          <Label fx:id="deleteOpt" text="Delete">
                                             <font>
                                                <Font name="System Bold" size="18.0" />
                                             </font>
                                             <cursor>
                                                <Cursor fx:constant="HAND" />
                                             </cursor>
                                          </Label>
                                       </children>
                                       <VBox.margin>
                                          <Insets top="5.0" />
                                       </VBox.margin>
                                       <padding>
                                          <Insets left="5.0" />
                                       </padding>
                                    </VBox>
                                 </children>
                                 <StackPane.margin>
                                    <Insets top="38.0" />
                                 </StackPane.margin>
                              </VBox>
                           </children>
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </StackPane>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
